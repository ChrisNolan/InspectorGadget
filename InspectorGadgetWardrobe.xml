<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
	    <Button name="InspectorGadgetWardrobeItemSlotButtonTemplate" inherits="ItemButtonTemplate" virtual="true">
        <Scripts>
            <OnLoad>
				IGWardrobeItemSlotButton_OnLoad(self);
			</OnLoad>
            <OnEvent>
				--InspectorGadgetWardrobeItemSlotButton_OnEvent(self, event, ...);
			</OnEvent>
            <OnClick>
				HandleModifiedItemClick(self.itemLink);
			</OnClick>
			<OnUpdate>
				CursorOnUpdate(self);
				if ( GameTooltip:IsOwned(self) ) then
					IGWardrobeItemSlotButton_OnEnter(self);
				end
			</OnUpdate>
            <OnEnter>
				IGWardrobeItemSlotButton_OnEnter(self, motion);
			</OnEnter>
            <OnLeave>
				GameTooltip:Hide();
				ResetCursor();
			</OnLeave>
        </Scripts>
		<NormalTexture file="Interface\Buttons\UI-Quickslot2">
			<Size>
				<AbsDimension x="64" y="64"/>
			</Size>
			<Anchors>
				<Anchor point="CENTER">
					<Offset>
						<AbsDimension x="0" y="-1"/>
					</Offset>
				</Anchor>
			</Anchors>
		</NormalTexture>
    </Button>
	<Button name="InspectorGadgetWardrobeItemSlotButtonLeftTemplate" inherits="InspectorGadgetWardrobeItemSlotButtonTemplate" virtual="true">
		<Layers>
